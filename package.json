{
  "name": "react-forms-input",
  "version": "1.0.7",
  "description": "Basic form components for React",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "react-basic-icon": "^1.0.8"
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "keywords": [
    "react",
    "forms",
    "input"
  ],
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/react-forms-input.git"
  },
  "homepage": "https://github.com/wtesler/react-forms-input"
}
